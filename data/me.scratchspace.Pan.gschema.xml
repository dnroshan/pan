<?xml version="1.0" encoding="UTF-8"?>
<schemalist gettext-domain="pan">
	<schema id="me.scratchspace.Pan" path="/me/scratchspace/Pan/">
	  <key name="radius" type="i">
	    <range min="1" max="100"/>
	    <default>10</default>
	  </key>
	  <key name="alpha" type="d">
	    <range min="0.1" max="1.0"/>
	    <default>0.6</default>
	  </key>
	  <key name="color" type="(ddd)">
	    <default>(1.0, 1.0, 1.0)</default>
	  </key>
	  <key name="size" type="(uu)">
	    <default>(640, 640)</default>
	  </key>
	</schema>
</schemalist>
